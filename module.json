{
    "name": "mana-base",
    "title": "Patria Mori Mana Base module",
    "description": "A simple module that provides a lot of nice automation for using Lori's mana system in Foundry VTT.",
    "authors": [
        {
            "name": "Jonas Drageset Lindaas"
        }
    ],
    "version": "0.2.1",
    "compatibility": {
        "minimum": 10,
        "verified": 10,
        "maximum": 11
    },
    "scripts": [
        "scripts/moduleConfig.js",
        "scripts/moduleHooks.js",
        "scripts/api/manaState.js",
        "scripts/model/manaAttributes.js",
        "scripts/model/manaSystemRules.js",
        "scripts/module/moduleSettings.js",
        "scripts/utils/actorUtils.js",
        "scripts/utils/flagUtils.js",
        "scripts/utils/moduleUtils.js",
        "scripts/utils/updateUtils.js",
        "scripts/view/actorSheetUI.js"
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "languages/en.json"
        }
    ],
    "styles": [
        "styles/mana-base.css"
    ],
    "url": "https://github.com/Patria-Mori/mana-base",
    "manifest": "https://raw.githubusercontent.com/Patria-Mori/mana-base/master/module.json",
    "download": "https://github.com/Patria-Mori/mana-base/releases/download/v0.2.1/module.zip",
    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
                "compatibility": {
                    "verified": "2.1.5"
                }
            }
        ],

        "requires": [
            {
                "id": "dae",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/dae/raw/v10/package/module.json",
                "compatibility": {
                    "verified": "0.10.22"
                }
            }
        ]
    }
}