{
    "name": "mana-base",
    "title": "Patria Mori Mana module",
    "description": "A simple module that provides a lot of nice automation for using Lori's mana system in Foundry VTT.",
    "authors": [
        {
            "name": "Jonas"
        }
    ],
    "version": "0.0.1",
    "compatibility": {
        "minimum": 10,
        "verified": 10,
        "maximum": 11
    },
    "scripts": [
        "scripts/mana.mjs",
        "scripts/manaHooks.js",
        "scripts/manaUtils.js",
        "scripts/manaState.js",
        "scripts/manaConfig.js"
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "languages/en.json"
        }
    ],
    "styles": [
        "styles/mana.css"
    ],
    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
                "compatibility": {
                    "verified": "2.1.5"
                }
            }
        ],

        "requires": [
            {
                "id": "dae",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/dae/-/raw/master/src/module.json",
                "compatibility": {
                    "verified": "0.10.22"
                }
            }
        ]
    }
}